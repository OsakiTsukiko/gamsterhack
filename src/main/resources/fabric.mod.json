{
  "schemaVersion": 1,
  "id": "gamster-hack",
  "version": "${version}",
  "name": "Gamster Hack",
  "description": "A Meteor Addon for the Gamster Anarchy Community",
  "authors": [
    "OsakiTsukiko"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/gamsterhack/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "osakitsukiko.gamsterhack.GamsterHack"
    ]
  },
  "mixins": [
    "gamsterhack.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
